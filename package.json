{
  "name": "formy-form",
  "version": "0.0.1",
  "description": "A Stenciljs powered form web component",
  "engines": {
    "node": "9.11.1",
    "npm": "6.4.0"
  },
  "module": "dist/esm/index.js",
  "main": "dist/formy.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "hyperform": "^0.9.16"
  },
  "devDependencies": {
    "@stencil/core": "^0.11.0",
    "@types/jest": "^23.3.1",
    "jest": "^23.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aemiro-allison/formy-form"
  },
  "author": "Aemiro Allison",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aemiro-allison/formy-form"
  },
  "homepage": "https://github.com/aemiro-allison/formy-form",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
